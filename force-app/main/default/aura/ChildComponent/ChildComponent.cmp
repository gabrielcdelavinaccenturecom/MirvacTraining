<aura:component >
    <!---type is the name of the event -->
    <aura:registerEvent name="cmpEvent" type="c:componentEvent" />
    <h1>Component Event Sample</h1>
    
    <p>
        <lightning:button label="Click here to fire a component event"
        onclick="{!c.fireComponentEvent}" />
    </p>
</aura:component>